<Window x:Class="DotaHelper2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DotaHelper2"
        mc:Ignorable="d"
        Title="Hero's builds" Height="600" Width="900" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Name="grid" Loaded="Grid_Loaded" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}">
        <Label Name="title" FontSize="24" Margin="0,5,0,506" HorizontalAlignment="Center" >Hero's build</Label>
        <ComboBox Name="comboB1" Margin="300,78,300,459" ItemsSource="{Binding Path=_dataCBList}"/>
        <Button Name="button1" Margin="384,135,384,397" FontSize="16" Click="Button_ClickAsync">Confirm</Button>
        <Button Name="button2" FontSize="12" Margin="27,25,822,506" RenderTransformOrigin="0.411,-2.647" Click="Button_Click_1">L/D</Button>
        <Label Name="build1" FontSize="12" Margin="0,220,600,0" RenderTransformOrigin="5.027,5.537" Visibility="Hidden">No data</Label>
        <Label Name="build2" FontSize="12" Margin="300,220,300,0" RenderTransformOrigin="5.027,5.537" Visibility="Hidden">No data</Label>
        <Label Name="build3" FontSize="12" Margin="600,220,0,0" RenderTransformOrigin="5.027,5.537" Visibility="Hidden">No data</Label>
    </Grid>
</Window>
